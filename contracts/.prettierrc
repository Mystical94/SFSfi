{
  "overrides": [
    {
      "files": ["*.js", "*.ts", "*.md"],
      "options": {
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "bracketSpacing": true,
        "trailingComma": "es5"
      }
    },
    {
      "files": ["*.js", "*.ts", "*.json"],
      "options": {
        "useTabs": false
      }
    },
    {
      "files": "*.md",
      "options": {
        "useTabs": false
      }
    }
  ]
}
